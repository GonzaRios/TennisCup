    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>777d1d0a-e0bc-48b9-99e6-8fde8002be3f</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
